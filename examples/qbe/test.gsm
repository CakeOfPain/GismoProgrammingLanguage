// Define a greeting function
greetUser := fun() {
    puts("Hello, user! Welcome to this generated code!")
}

// Define a function to show some ASCII art
showArt := fun() {
    puts("Here is some ASCII art:")
    puts("  /\\\\_/\\\\")
    puts(" ( o.o )")
    puts("  > ^ <")
}

// Define a function to perform simple computations and print them
showComputedMessage := fun() {
    puts("Performing some computations:")
    puts("42 + 13 = " + (42 + 13))
    puts("'Hello' + 'World' = " + ("Hello" + "World"))
}

// The main function calls the other functions
main := fun() {
    greetUser()
    showArt()
    showComputedMessage()
}